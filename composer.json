{
	"name": "vbuilder/cli",
	"type": "library",

	"description": "vBuilder Framework / CLI",
	"keywords": ["vbuilder", "cli"],
	"homepage": "http://www.vbuilder.cz",
	"authors": [
		{
			"name": "V3Net.cz, s.r.o.",
			"homepage": "http://www.v3net.cz"
		}
	],

	"require": {
		"php": ">=5.3.1",
		"vbuilder/utils": "~0.10.0"
	},

	"autoload": {
